@model FinalProject_MVC.Models.Empresa
@{
    ViewBag.Title = "Cadastrar";
}

<h2>Cadastrar</h2>
@if(TempData["msg"] != null)
{
    <div class="alert alert-success">@TempData["msg"]</div>
}

@using (Html.BeginForm())
{
    <div class="form-group">
        @Html.LabelFor(e => e.Cnpj)
        @Html.TextBoxFor(e => e.Cnpj, new { @class = "form-control"})
        @Html.ValidationMessageFor(e => e.Cnpj)
    </div>
    <div class="form-group">
        @Html.LabelFor(e => e.RazaoSocial)
        @Html.TextBoxFor(e => e.RazaoSocial, new { @class = "form-control" })
        @Html.ValidationMessageFor(e => e.RazaoSocial)
    </div>
    <input type="submit" value="Salvar" class="btn btn-primary"/>
}
